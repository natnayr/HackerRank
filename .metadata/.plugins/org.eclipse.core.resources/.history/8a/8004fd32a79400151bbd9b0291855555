import java.util.Scanner;


public class ExtraLongFactorials {

	public static final int nBox = 160;
	
	public static void main(String[] args) {
		//100! = 9.3..e+157, thus digit array will be 160 long max
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		
		int[] ans = new int[nBox];
	
		for(int i=0; i<nBox; i++){
			//init array
			ans[i] = 0;
		}
		
		//1
		ans[nBox-1] = 1;
		
		for(int i=0; i<=N; i++){
			ans = multiply(ans, i);
		}
		
		//print ans[]
		boolean zeros = true;
		for(int i=0; i<nBox; i++){
			if(zeros == true && ans[i] > 0){
				zeros = false;
			}
			if(zeros == false){
				System.out.print(ans[i]);
			}
		}
	}
	
	
	public static int[] multiply(int[] ans, int by){
		
		int carry10s = 0;
		int carry100s = 0;
		//start from right box and multiply (carry over) to left
		for(int i=nBox-1; i>=0; i--){
			int miniAns = (ans[i] * by) + carry10s;
			
			ans[i] = miniAns % 10;
			carry10s = miniAns/10;
			carry100s = miniAns/100;

			
		}
		
		//as N is 1 <= N <= 100, 
		return ans;
	}
}
