import java.util.Scanner;


public class TaumAndBday {

	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		int T = in.nextInt();
		while(T > 0){
			
			int numB = in.nextInt();
			int numW = in.nextInt();
			int X = in.nextInt();
			int Y = in.nextInt();
			int Z = in.nextInt();
			
			int justB = numB * X;
			int transB = (numB * Y) + (numB * Z);
			int costB = Math.min(justB, transB);
			
			T--;
		}
	}
	
}
