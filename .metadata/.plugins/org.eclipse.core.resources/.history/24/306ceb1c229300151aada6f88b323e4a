import java.util.Scanner;


public class CavityMap {

	public static void main(String[] args){
		
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int[][] grid = new int[n][n];
		for(int i=0; i<n; i++){
			for(int j=0; j<n; j++){
				grid[i][j] = in.nextInt();
			}
		}
		
		for(int i=1; i<n-1; i++){
			for(int j=1; j<n-1; j++){
				int cavCell = grid[i][j];
				if(grid[i-1][j] < calCell && grid[i+1][j] < calCell &&
						grid[i][j-1] < calCell && grid[i][j+1] < cavCell){
					
				}
			}
		}
	}
	
}
