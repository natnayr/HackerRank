import java.util.Scanner;


public class CutTheSticks {
	
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		
		int N = in.nextInt();
		int[] arr = new int[N];
		
		int length = 1000;
		//init jumpArr running numbers & input arr
		for(int i=0; i<N; i++){
			arr[i] = in.nextInt();
			
			if(arr[i] < length){
				length = arr[i];
			}
		}
		
		
		int smallest = 1000;
		boolean cut = true;
		
		//walk through and start
		while(cut){
			for(int i=0; i<N; i++){
				if(i==0){
					smallest = 1000;
					prevIndex = 0;
					cut = false;
				}
				
				if(arr[i] >= length){
					arr[i] -= length;
					cut = true;
					
					if(arr[i] < smallest){
						smallest = arr[i];
					}
				}
			}
		}
	}
	
}
